


# WriteValue![%Q:,Q%]
val: {%Q?&T:CommandQueue<&T>%}
!Enqueue
var ec := cl.EnqueueWriteBuffer(
  cq, o.Native, clBool.NON_BLOCKING,
  UIntPtr.Zero, new UIntPtr(Marshal.SizeOf(default(T))),
  !val:ptr!,
  !evs!
);
OpenCLABCInternalException.RaiseIfError(ec);










# [%mode:Write,Read%]NativeValue[%Area:Area,%]
native_data: CommandQueue<NativeValue{%Area%}<&T>>
!Enqueue
var ec := cl.Enqueue{%mode%}Buffer(
  cq, o.Native, clBool.NON_BLOCKING,
  UIntPtr.Zero, !native_data!{%Area?:.Area%}.ByteSize,
  !native_data!{%Area?:.Area%}.ptr,
  !evs!
);
OpenCLABCInternalException.RaiseIfError(ec);


