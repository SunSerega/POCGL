      except
        on e: OpenCLException do
          res += e.Code.ToString;
      end;
      res += #10;
      res += 'HostTimerResolution = ';
      try
        res += _ObjectToString(HostTimerResolution);
      except
        on e: OpenCLException do
          res += e.Code.ToString;
      end;
    end;
    public function ToString: string; override;
    begin
      var res := new StringBuilder;
      ToString(res);
      Result := res.ToString;
    end;
    
