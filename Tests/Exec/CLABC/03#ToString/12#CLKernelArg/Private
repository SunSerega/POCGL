


TestRange('Managed', ()->
begin
TestRange('Array', ()->
begin
Test(CLKernelArgPrivate.FromArray(CQ(a)));
TestT&<CLKernelArgPrivate>(a);
TestT&<CLKernelArgPrivate>(CQ(a));
TestT&<CLKernelArgPrivate>(new ConstQueue<array of T>(a));
TestT&<CLKernelArgPrivate>(new ParameterQueue<array of T>('a', a));
end);
TestRange('Array2', ()->
begin
Test(CLKernelArgPrivate.FromArray2(CQ(a2)));
TestT&<CLKernelArgPrivate>(a2);
TestT&<CLKernelArgPrivate>(CQ(a2));
TestT&<CLKernelArgPrivate>(new ConstQueue<array[,] of T>(a2));
TestT&<CLKernelArgPrivate>(new ParameterQueue<array[,] of T>('a2', a2));
end);
